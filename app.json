{
  "expo": {
    "name": "panda-doctor",
    "slug": "panda-doctor",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "pandadoctor",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pandahealth.doctor"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.pandahealth.doctor",
      "googleServicesFile": "./android/app/google-services.json"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/pandahealth-doctor-logo.png",
          "imageWidth": 250,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/pandahealth-doctor-logo.png",
            "backgroundColor": "#000000"
          }
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/messaging"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "env": {
        "production": {
          "apiBaseUrl": "https://pandahealthapi-linux-cbb2faancjdre6ae.westeurope-01.azurewebsites.net/api"
        },
        "development": {
          "apiBaseUrl": "https://pandahealthapi-linux-cbb2faancjdre6ae.westeurope-01.azurewebsites.net/api"
        }
      },
      "router": {},
      "eas": {
        "projectId": "132b2e5c-3578-4f3c-8aff-392555cd8e7e"
      }
    }
  }
}
